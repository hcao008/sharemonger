<div class="show">
  <%= simple_form_for(@order) do |f| %>
    <%= f.error_notification %>

    <div class="form-inputs">
      <%= f.input :feedback_rating %>
      <%= f.input :feedback_comment %>
    </div>

    <div class="form-actions">
      <%= f.button :submit %>
    </div>
  <% end %>
</div>
